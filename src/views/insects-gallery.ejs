<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <!-- Removed initial-scale and max-width from content attribute to fix zooming problems on mobile -->
    <title><%= title %></title>
    <link href="../style.css" rel="stylesheet" />
    <script src="../index.js" defer="defer"></script>
  </head>

  <body>
    <div class="scroll-wrapper">
      <main class="main gal-main" data-name="insects-gallery">
        <%- include('header.ejs') %>
        <!-- INSECTS GALLERY -->
        <section class="section gal-portrait" id="insects-gallery" hidden>
          <div class="gallery portrait-gallery">
            <%- include ('portrait-animation-ctrl.ejs') %>

            <div
              class="slider-box portrait-slider-box"
              tabindex="0"
              role="region"
              aria-roledescription="photo slider"
              aria-label="Insects Gallery"
              hidden
            >
              <div class="img-display-container">
                <button type="button" class="back-arrow" data-back-arrow>
                  <img
                    data-icon-back
                    class="arrow"
                    src="../assets/imgs/icons/reshot-icon-arrow-chevron-left-975UQXVKZF.svg"
                    alt="Back Arrow"
                  />
                </button>

                <div class="photo-box portrait-photo-box">
                  <!-- BEE ON SAGE FLOWER -->
                  <picture
                    class="picture"
                    aria-role="group"
                    aria-roledescription="slide"
                    tabindex="0"
                  >
                    <source
                      media="(min-width: 300px) and (max-width: 499px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.06.09_bee-vertical-250p.jpg
                      "
                    />

                    <source
                      media="(min-width: 500px) and (max-width: 750px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.06.09_bee-vertical-420p.jpg
                      "
                    />

                    <source
                      media="(min-width: 751px) and (max-width: 1020px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.06.09_bee-vertical-600p.jpg
                      "
                    />

                    <img
                      src="../assets/imgs/insects-gal/Guneratne_2023.06.09_bee-vertical-780p.jpg"
                      alt="Slide 1 of 6: A bee sitting on a purple sage flower with its arms over its head, as if about to thrust its arms forward into the flower."
                      id="slide1"
                    />
                  </picture>

                  <!-- CATERPILLAR -->
                  <picture
                    class="picture"
                    role="group"
                    aria-roledescription="slide"
                    tabindex="0"
                  >
                    <source
                      media="(min-width: 300px) and (max-width: 499px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.09.05_caterpillar-vertical-250p.jpg
                      "
                    />

                    <source
                      media="(min-width: 500px) and (max-width: 750px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.09.05_caterpillar-vertical-420p.jpg
                      "
                    />

                    <source
                      media="(min-width: 751px) and (max-width: 1020px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.09.05_caterpillar-vertical-600p.jpg
                      "
                    />

                    <img
                      src="../assets/imgs/insects-gal/Guneratne_2023.09.05_caterpillar-vertical-780p.jpg"
                      alt="Slide 2 of 6: A close up of a swallowtail butterfly caterpillar crawling vertically up a fennel stalk. The caterpillar is green with black stripes and yellow dots on every other stripe."
                      id="slide2"
                    />
                  </picture>

                  <!-- DRAGONFLY -->
                  <picture
                    class="picture"
                    role="group"
                    aria-roledescription="slide"
                    tabindex="0"
                  >
                    <source
                      media="(min-width: 300px) and (max-width: 499px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.08.02_dragonfly-vertical-250p.jpg
                      "
                    />

                    <source
                      media="(min-width: 500px) and (max-width: 750px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.08.02_dragonfly-vertical-420p.jpg
                      "
                    />

                    <source
                      media="(min-width: 751px) and (max-width: 1020px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.08.02_dragonfly-vertical-600p.jpg
                      "
                    />

                    <img
                      src="../assets/imgs/insects-gal/Guneratne_2023.08.02_dragonfly-vertical-780p.jpg"
                      alt="Slide 3 of 6: An eastern amberwing dragonfly sits on the branch of a cotoneaster plant, facing right in the frame and glancing toward the viewer."
                      id="slide3"
                    />
                  </picture>

                  <!-- ANT -->
                  <picture
                    class="picture"
                    role="group"
                    aria-roledescription="slide"
                    tabindex="0"
                  >
                    <source
                      media="(min-width: 300px) and (max-width: 499px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.06.09_ant-leaf-vertical-250p.jpg
                      "
                    />

                    <source
                      media="(min-width: 500px) and (max-width: 750px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.06.09_ant-leaf-vertical-420p.jpg
                      "
                    />

                    <source
                      media="(min-width: 751px) and (max-width: 1020px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.06.09_ant-leaf-vertical-600p.jpg
                      "
                    />

                    <img
                      src="../assets/imgs/insects-gal/Guneratne_2023.06.09_ant-leaf-vertical-780p.jpg"
                      alt="Slide 4 of 6: A black ant crawls toward the left side of a large green leaf with serrated edges that is pointing vertically toward the top of the frame. The majority of the frame is filled with light and dark green serrated leaves from the plant."
                      id="slide4"
                    />
                  </picture>

                  <!-- BUTTERFLY -->
                  <picture
                    class="picture"
                    role="group"
                    aria-roledescription="slide"
                    tabindex="0"
                  >
                    <source
                      media="(min-width: 300px) and (max-width: 499px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.08.09_butterfly-vertical-250p.jpg
                      "
                    />

                    <source
                      media="(min-width: 500px) and (max-width: 750px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.08.09_butterfly-vertical-420p.jpg
                      "
                    />

                    <source
                      media="(min-width: 751px) and (max-width: 1020px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.08.09_butterfly-vertical-600p.jpg
                      "
                    />

                    <img
                      src="../assets/imgs/insects-gal/Guneratne_2023.08.09_butterfly-vertical-780p.jpg"
                      alt="Slide 5 of 6: A bright yellow eastern tiger swallowtail butterfly with black stripes sits on top of a purple thistle flower."
                      id="slide5"
                    />
                  </picture>

                  <!-- GRASSHOPPER -->
                  <picture
                    class="picture"
                    role="group"
                    aria-roledescription="slide"
                    tabindex="0"
                  >
                    <source
                      media="(min-width: 300px) and (max-width: 499px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.08.26_grasshopper-vertical-250p.jpg
                      "
                    />

                    <source
                      media="(min-width: 500px) and (max-width: 750px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.08.26_grasshopper-vertical-420p.jpg
                      "
                    />

                    <source
                      media="(min-width: 751px) and (max-width: 1020px) and (orientation: portrait)"
                      srcset="
                        ../assets/imgs/insects-gal/Guneratne_2023.08.26_grasshopper-vertical-600p.jpg
                      "
                    />

                    <img
                      src="../assets/imgs/insects-gal/Guneratne_2023.08.26_grasshopper-vertical-780p.jpg"
                      alt="Slide 6 of 6: A closeup of a grasshopper sitting on a serrated leaf facing upward toward the right at a diagonal angle in the frame."
                      id="slide6"
                    />
                  </picture>
                </div>

                <button type="button" class="forward-arrow" data-forward-arrow>
                  <img
                    data-icon-forward
                    class="arrow"
                    src="../assets/imgs/icons/reshot-icon-arrow-chevron-right-WDGHUKQ634.svg"
                    alt="Forward Arrow"
                  />
                </button>
              </div>

              <div class="img-dots-container">
                <div class="img-dots">
                  <button
                    type="button"
                    class="circle-icon-div"
                    data-circle="0"
                    aria-label="Select Slide 1 of 6."
                  >
                    <div class="circle-icon" data-circle="0"></div>
                  </button>

                  <button
                    type="button"
                    class="circle-icon-div"
                    data-circle="1"
                    aria-label="Select Slide 2 of 6."
                  >
                    <div class="circle-icon" data-circle="1"></div>
                  </button>

                  <button
                    type="button"
                    class="circle-icon-div"
                    data-circle="2"
                    aria-label="Select Slide 3 of 6."
                  >
                    <div class="circle-icon" data-circle="2"></div>
                  </button>

                  <button
                    type="button"
                    class="circle-icon-div"
                    data-circle="3"
                    aria-label="Select Slide 4 of 6."
                  >
                    <div class="circle-icon" data-circle="3"></div>
                  </button>

                  <button
                    type="button"
                    class="circle-icon-div"
                    data-circle="4"
                    aria-label="Select Slide 5 of 6."
                  >
                    <div class="circle-icon" data-circle="4"></div>
                  </button>

                  <button
                    type="button"
                    class="circle-icon-div"
                    data-circle="5"
                    aria-label="Select Slide 6 of 6."
                  >
                    <div class="circle-icon" data-circle="5"></div>
                  </button>
                </div>
              </div>
            </div>
          </div>
        </section>

        <section class="section gal-landscape" id="insects-gallery" hidden>
